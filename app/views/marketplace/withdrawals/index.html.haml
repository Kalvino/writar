= heading "Payment History"
.row
  .col-md-12
    .portlet
      .portlet-header
        %h3
          %i.fa.fa-history
          Payment History
      / /.portlet-header
      .portlet-content
        .table-responsive
          %table#withdrawals.table.table-striped.table-hover.table-highlight{"data-seller-id" => "#{encrypt(current_seller.id)}", "data-source" => "#{marketplace_withdrawals_path(format: "json")}"}
            %thead
              %tr
                %th Date
                %th User
                %th Amount
            %tbody
        / /.table-responsive
      / /.portlet-content
    / /.portlet
  / /.col
